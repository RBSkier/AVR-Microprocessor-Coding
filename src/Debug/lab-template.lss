0000e9 ef4f                      	ldi temp0, 0xFF
                                   kri_delay:
0000ea 954a                      	dec temp0
0000eb f7f1                      	brne kri_delay
                                 
0000ec b146                      	in	temp0, PINC				; read PORTC
0000ed 704f                      	andi temp0, ROWMASK
0000ee 304f                      	cpi temp0, 0xF				; check if any rows are on
0000ef f059                      	breq kri_nextcol
                                 								; if yes, find which row is on
0000f0 e021                      	ldi r18, INITROWMASK		; initialise row check
0000f1 2700                      	clr	r16						; initial row
                                   kri_rowloop:
0000f2 3004                      	cpi r16, 4
0000f3 f039                      	breq kri_nextcol
0000f4 2f54                      	mov temp1, temp0
0000f5 2352                      	and temp1, r18				; check masked bit
0000f6 f041                      	breq kri_convert 				; if bit is clear, convert the bitcode
0000f7 9503                      	inc r16						; else move to the next row
0000f8 0f22                      	lsl r18					; shift the mask to the next bit
0000f9 940c 00f2                 	jmp kri_rowloop
                                 
                                   kri_nextcol:
0000fb 0f33                      	lsl r19					; else get new mask by shifting and 
0000fc 9513                      	inc r17						; increment column value
0000fd 940c 00e6                 	jmp kri_colloop					; and check the next column
                                 
                                   kri_convert:
0000ff 3013                        	cpi r17,3
000100 f071                      	breq kri_letters			
000101 3003                      	cpi r16, 3					; if row is 3 we have a symbol or 0
000102 f079                      	breq kri_symbols
000103 3002                      	cpi r16, 2
000104 f039                      	breq kri_none
