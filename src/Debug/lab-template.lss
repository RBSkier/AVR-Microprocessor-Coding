00010c e040                          ldi temp0,0
00010d 940c 011a                 	jmp kri_convert_end          ;keypad button named"7""8""9" are not used
                                 
                                   kri_letters:
00010f e040                      	ldi temp0, 0
000110 940c 011a                 	jmp kri_convert_end
                                 
                                   kri_symbols:
