000106 0f44                      	lsl temp0
000107 0f40                      	add temp0, r16				; temp1 = row * 3
000108 0f41                      	add temp0, r17				; add the column address to get the value
000109 5f42                      	subi temp0, -14  			
00010a 940c 011a                 	jmp kri_convert_end
                                 
